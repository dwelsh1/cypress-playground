{
    "viewportWidth": 1920,
    "viewportHeight": 1080,
    "chromeWebSecurity": false,
    "env": {
        "grepFilterSpecs": true,
        "grepOmitFiltered": true,
        "browserPermissions": {
          "notifications": "allow",
          "geolocation": "allow",
          "camera": "block",
          "microphone": "block",
          "images": "allow",
          "javascript": "allow",
          "popups": "ask",
          "plugins": "ask",
          "cookies": "allow"
        }
    }
}
